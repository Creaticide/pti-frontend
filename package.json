{
  "name": "oskari-frontend-paikkis",
  "version": "1.8.0",
  "description": "kartta.paikkatietoikkuna.fi bundles & app conf",
  "keywords": [
    "Oskari"
  ],
  "dependencies": {
    "cesium": "^1.58.0",
    "olcs": "^2.9.0",
    "oskari-frontend": "git+https://git@github.com/oskariorg/oskari-frontend.git#develop",
    "oskari-frontend-contrib": "git+https://git@github.com/oskariorg/oskari-frontend-contrib.git#develop"
  },
  "scripts": {
    "dev-mode": "node ./node_modules/oskari-frontend/scripts/oskari-dev-mode",
    "dev-mode:off": "npm run dev-mode -- disabled",
    "dev-mode:on": "npm run dev-mode -- enabled",
    "dev-mode:disable": "npm run dev-mode:on && npm run dev-mode:clean && npm i git+https://git@github.com/oskariorg/oskari-frontend.git#develop && npm i git+https://git@github.com/oskariorg/oskari-frontend-contrib.git#develop && npm i",
    "dev-mode:enable": "npm run dev-mode:off && npm run dev-mode:available && npm run dev-mode:clean && npm i ../oskari-frontend && npm i ../oskari-frontend-contrib && npm i",
    "dev-mode:available": "node ./node_modules/oskari-frontend/scripts/oskari-dev-mode-requirements",
    "dev-mode:clean": "(rm -r node_modules || true) && (rm package-lock.json || true)",
    "build": "npm run dev-mode:off && webpack --config ./node_modules/oskari-frontend/webpack.config.js --mode production --progress --env.appdef=applications/paikkatietoikkuna.fi --env.theme=theme.less",
    "start": "npm run dev-mode:off && webpack-dev-server --config ./node_modules/oskari-frontend/webpack.config.js --hot --env.appdef=applications/paikkatietoikkuna.fi --env.theme=theme.less",
    "build:dev": "npm run dev-mode:on && node ./node_modules/oskari-frontend/node_modules/webpack/bin/webpack.js --config ./node_modules/oskari-frontend/webpack.config.js --mode production --progress --env.appdef=applications/paikkatietoikkuna.fi --env.theme=theme.less",
    "start:dev": "npm run dev-mode:on && node ./node_modules/oskari-frontend/node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./node_modules/oskari-frontend/webpack.config.js --hot --env.appdef=applications/paikkatietoikkuna.fi --env.theme=theme.less",
    "sprite": "node ./node_modules/oskari-frontend/webpack/sprite.js",
    "test": "eslint .",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "private": true,
  "devDependencies": {
    "eslint": "5.15.3",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "fs-extra": "^8.0.1"
  }
}
